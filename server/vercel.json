{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "models/**/*", "use": "@vercel/node" }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/orders",
      "dest": "models/orders.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
